<div class='card'>
  <div class='card-body'>
    <h5 class='card-title'><%= user.name %></h5>
    <h6 class='1card-subtitle mb-2 text-muted1'><%= user.iduff %></h6>
    <p><%= user.cpf %></p>
  </div>
  <div class="card-footer">
    <%= link_to 'Editar', edit_user_path(user), method: :put, class: 'btn btn-warning' %>
    <%= link_to 'Deletar', user, method: :delete, data: {confirm: "Tem certeza?"}, class: 'btn btn-danger'%>
  </div>
</div>